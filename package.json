{
  "name": "Mtatitra",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "start": "npx ts-node ./src/index.ts",
    "preinstall": "npm install -g npm-run-all",
    "install-all": "npm-run-all --parallel install-client install-backoffice",
    "install-client": "(cd ./Mtatitra-Front && npm install)",
    "install-backoffice": "(cd ./Mtatitra-backoffice && npm install)",
    "heroku-postbuild": "npm-run-all --parallel build postbuild",
    "postbuild": "npx ts-node ./src/postbuild.ts",
    "dev": "npx ts-node-dev --inspect=4321 --poll --clear ./src/index.ts",
    "build-client": "cd Mtatitra-Front && npx ng build --prod",
    "build-backoffice": "cd Mtatitra-backoffice && npx ng build --prod --base-href /admin/",
    "client": "npm-run-all --sequential install-client build-client",
    "backoffice": "npm-run-all --sequential install-backoffice build-backoffice",
    "build": "npm-run-all --parallel client backoffice"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/node": "^8.0.29",
    "@types/nodemailer": "^6.4.0",
    "bcrypt": "^4.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "hasha": "^5.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "nodemailer": "^6.4.3",
    "nodemailer-smtp-transport": "^2.7.4",
    "pg": "^7.15.1",
    "reflect-metadata": "^0.1.10",
    "socket.io": "^2.3.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typeorm": "0.2.21",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "typeorm-fixtures-cli": "^1.6.0"
  },
  "cacheDirectories": [
    "Mtatitra-backoffice/node_modules",
    "Mtatitra-Front/node_modules",
    "node_modules"
  ]
}
